# A sample Gemfile
source "https://rubygems.org"

ruby '2.3.0'

gem 'sinatra'     # minimalistic ruby web framework
gem 'puma'        # threaded ruby web server
gem 'sass'        # Sass/SCSS support

gem 'hurley'
gem 'oga'
gem 'virtus'
gem 'lru_redux'

group :development do
	gem 'foreman'
	gem 'rspec'
end
